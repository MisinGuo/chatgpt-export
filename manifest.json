{
   "action": {
      "default_icon": {
         "128": "image128.png",
         "16": "image16.png",
         "48": "image48.png"
      },
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "https://*.openai.com/*", "https://chatgpt.com/*", "https://grok.com/*", "https://gemini.google.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "Export chat history from ChatGPT and Grok websites to normal format as Markdown, which can be opened via typora exactly.",
   "key": "",
   "manifest_version": 3,
   "name": "ChatGPT to MarkDown plus",
   "permissions": [  ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "5.5"
}
